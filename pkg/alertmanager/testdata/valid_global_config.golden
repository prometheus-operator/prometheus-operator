global:
  resolve_timeout: 30s
  http_config:
    oauth2:
      client_id: clientID
      client_secret: clientSecret
      scopes:
      - any
      token_url: https://test.com
      endpoint_params:
        some: value
    follow_redirects: true
  smtp_from: from
  smtp_hello: smtp.example.org
  smtp_smarthost: smtp.example.org:587
  smtp_auth_username: dev@smtp.example.org
  smtp_auth_password: password
  smtp_auth_secret: secret
  smtp_auth_identity: dev@smtp.example.org
  smtp_require_tls: true
  smtp_tls_config:
    insecure_skip_verify: true
    min_version: TLS12
    max_version: TLS13
route:
  receiver: mynamespace/global-config/null
  routes:
  - receiver: mynamespace/global-config/myreceiver
    matchers:
    - mykey="myvalue"
    - mykey1="myvalue1"
receivers:
- name: mynamespace/global-config/null
- name: mynamespace/global-config/myreceiver
- name: mynamespace/global-config/jira
  jira_configs:
  - send_resolved: true
    api_url: https://test.com
    project: projectA
    summary: summary
    description: description
    labels:
    - aa
    - bb
    priority: priority
    issue_type: bug
    resolve_transition: ResolveTransition
    reopen_transition: ReopenTransition
    wont_fix_resolution: WontFixResolution
    reopen_duration: 5s
    fields:
      customField1: '{"aa": "recv2", "bb": 11}'
      customField2: ""
      customField3: '[{"aa": "recv2", "bb": 11, "cc": {"aa": 11}}, "aa", 11, ["aa",
        "bb", 11] ]'
templates: []
