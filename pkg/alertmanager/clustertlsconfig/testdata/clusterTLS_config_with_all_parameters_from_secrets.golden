tls_server_config:
  key_file: /cluster_tls_certs_path_prefix/server_tls/secret/test-secret-key
  cert_file: /cluster_tls_certs_path_prefix/server_tls/secret/test-secret-cert
  client_auth_type: RequireAnyClientCert
  client_ca_file: /cluster_tls_certs_path_prefix/server_tls/secret/test-secret-ca
  min_version: TLS11
  max_version: TLS13
  cipher_suites:
  - cipher-1
  - cipher-2
  prefer_server_cipher_suites: false
  curve_preferences:
  - curve-1
  - curve-2
tls_client_config:
  key_file: /cluster_tls_certs_path_prefix/client_tls/secret/test-secret-key
  cert_file: /cluster_tls_certs_path_prefix/client_tls/secret/test-secret-cert
  ca_file: /cluster_tls_certs_path_prefix/client_tls/secret/test-secret-ca
  insecure_skip_verify: true
