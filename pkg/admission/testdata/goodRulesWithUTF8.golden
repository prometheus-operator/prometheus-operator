      "spec": {
        "groups": [
          {
            "name": "test.utf8.rules",
            "rules": [
              {
                "alert": "TestUTF8Alert",
                "annotations": {
                  "message": "UTF-8 test rule with émojis 🚀 and ünicöde",
                  "description": "测试 UTF-8 characters in annotations"
                },
                "expr": "up{app=\"myapp\",service=\"测试服务\"} > 0",
                "for": "5m",
                "labels": {
                  "severity": "critical",
                  "team": "platform",
                  "测试标签": "测试_label_with_中文"
                }
              },
              {
                 "record": "http.requests.rate_5m",
                 "expr": "sum by(service,job,\"http.request\") (rate({\"http.requests.total\",\"error.type\"!=\"\"}[5m]))",
                "labels": {
                  "type": "recording",
                  "描述": "Recording rule with UTF-8 characters 🎯"
                }
              }
            ]
          }
        ]
      }
    },
