global:
  evaluation_interval: 30s
  scrape_interval: 30s
  external_labels:
    prometheus: default/test
    prometheus_replica: $(POD_NAME)
scrape_configs: []
remote_write:
- url: http://example.com
  remote_timeout: 30s
  proxy_url: http://no-proxy.com
  no_proxy: 0.0.0.0
  proxy_from_environment: false
  proxy_connect_header:
    header:
    - value
  follow_redirects: true
