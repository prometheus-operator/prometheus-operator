global:
  scrape_interval: 30s
  external_labels:
    prometheus: default/test
    prometheus_replica: $(POD_NAME)
  evaluation_interval: 30s
scrape_configs:
- job_name: scrapeConfig/default/testscrapeconfig1
  http_sd_configs:
  - tls_config:
      ca: ca
      cert: cert
      key: private-key
    url: http://localhost:9100/sd.json
  relabel_configs:
  - source_labels:
    - job
    target_label: __tmp_prometheus_job_name
