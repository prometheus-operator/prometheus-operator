global:
  scrape_interval: 30s
  external_labels:
    prometheus: default/test
    prometheus_replica: $(POD_NAME)
  evaluation_interval: 30s
scrape_configs:
- job_name: scrapeConfig/default/testscrapeconfig1
  stackit_sd_config:
  - authorization:
      type: Bearer
      credentials: value
    oauth2:
      client_id: client-id
      client_secret: client-secret
      token_url: http://test.url
      scopes:
      - scope 1
      - scope 2
      endpoint_params:
        param1: value1
        param2: value2
    project: 11111111-1111-1111-1111-111111111111
    region: eu01
    endpoint: https://api.stackit.de/
  relabel_configs:
  - source_labels:
    - job
    target_label: __tmp_prometheus_job_name
