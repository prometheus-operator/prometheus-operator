global:
  evaluation_interval: 30s
  scrape_interval: 30s
  external_labels:
    prometheus: default/test
    prometheus_replica: $(POD_NAME)
scrape_configs:
- job_name: scrapeConfig/default/testscrapeconfig1
  consul_sd_configs:
  - authorization:
      type: Bearer
      credentials: secret
    server: localhost:8500
  relabel_configs:
  - source_labels:
    - job
    target_label: __tmp_prometheus_job_name
