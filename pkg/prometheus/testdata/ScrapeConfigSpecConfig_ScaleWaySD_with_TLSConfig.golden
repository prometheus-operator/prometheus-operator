global:
  scrape_interval: 30s
  external_labels:
    prometheus: default/test
    prometheus_replica: $(POD_NAME)
  evaluation_interval: 30s
scrape_configs:
- job_name: scrapeConfig/default/testscrapeconfig1
  scaleway_sd_configs:
  - access_key: AccessKey
    secret_key: value
    project_id: "1"
    role: instance
    tls_config:
      ca: ca
      cert: cert
      key: private-key
  relabel_configs:
  - source_labels:
    - job
    target_label: __tmp_prometheus_job_name
